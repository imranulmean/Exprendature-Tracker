{
    "version": 2,
    "builds": [
      {
        "src": "*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/",
        "headers": {
          "Access-Control-Allow-Origin": "https://exp-tracker-face.vercel.app",
          "Access-Control-Allow-Credentials": "true"
        }        
      }
    ]
}